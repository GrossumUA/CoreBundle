{
    "name": "grossum/core-bundle",
    "description": "This core bundle required for Grossum bundles",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Dmytro Boiko",
            "email": "dboyko@grossum.com"
        }
    ],
    "keywords": [
        "core bundle",
        "grossum"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd" : "@stable",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "Grossum\\CoreBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
